{
  "buildCommand": "cd client && pnpm install && pnpm build",
  "outputDirectory": "client/dist",
  "installCommand": "cd client && pnpm install",
  "framework": "vite",
  "devCommand": "cd client && pnpm dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}

